<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dingding.seckill.dao.CommodityTestMapper">

    <insert id="insertCommodity" parameterType="com.dingding.seckill.entity.Commodity">
        insert into commodity (commodity_id, name, number)
        values (#{commodityId},#{name},#{number})
    </insert>
</mapper>